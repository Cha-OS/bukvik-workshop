{
	"name": "Nabokov into English",
	"authors": "Eugenia Kelbert & Sasha Mile Rudan",
	"version": "0.1",
	"description": "Close analysis of Nabokov's self-translations into English: these are books mostly translated by the author with the help of a translator",
	
	"data": [
		{
			"type": "dataset",
			"namespace": "<NAMESPACE_DATA>.execute",
			"name": "defense-en",
			"paramsProcess": {
				"fileIn.name":  "E"
			},
			"params": {
				"fileIn.name": "<FOLDER_DATA>/zhenia-master-standard.json",
				"fileIn.type": "json",
				"fileIn.encoding": "utf-8",
				"environment": {
					"SUB_PROJECT_NAME": "defense-en",
					"NAMESPACE_DATA": "<NAMESPACE_DATA>.<SUB_PROJECT_NAME>",
					"NAMESPACE_TASK": "<NAMESPACE_TASK>.<SUB_PROJECT_NAME>"
				},
				"mappings": {
					"dataset:<NAMESPACE_DATA>:distribution-out['params']['fileOut.type']": ["html"],

					"task:<NAMESPACE_TASK>.parsers:Words['params']['paragraphs.separators']": ["[\\r\\n]+"],
					"task:<NAMESPACE_TASK>.pos:Parsing POS-external['params']['cmd']": "<TREE_TAGGER_CMD_ENGLISH>",
					"task:<NAMESPACE_TASK>.pos:ReMapping POS-tags['params']['language']": "english",
					"task:<NAMESPACE_TASK>.stats:Calculating Simple Stats['ports']['corpus']['datasets']": 
						[
							{
								"namespace": "corpus.english.brown",
								"name": "distribution"
							}
						]
				}
			},
			"shouldInvalidate": true
		},
		{
			"type": "task",
			"namespace": "<NAMESPACE_TASK>.execute",
			"name": "defense-en",
			"comment": "Executing Uppsala sub-experiment",
			"processorNsN": "task-executors.TaskListExecutor.TaskListExecutor",
			"execution": 0,
			"forceExecute": true,
			"isPower": true,
			"ports": {
				"in": {
					"direction": "in",
					"datasets": [
						{
							"namespace": "<NAMESPACE_DATA>.execute",
							"name": "defense-en"
						}
					]
				}
			}
		},
