					"task:<NAMESPACE_TASK>.pos:ReMapping POS-tags['params']['language']": "russian",
					"task:<NAMESPACE_TASK>.stats:Calculating Simple Stats['ports']['corpus']['datasets']": 
						[
							{
								"namespace": "corpus.russian.national",
								"name": "distribution"
							}
						]
				}
			},
			"shouldInvalidate": false
		},
